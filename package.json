{
  "name": "vue-3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "mock": "json-server --watch ./mock/data.json --port 10002",
    "lint": "eslint 'src/**/*.{js,vue}'",
    "dev": "vite",
    "build:v": "vite build",
    "build": "rm -rf dist && cross-env NODE_ENV=production webpack --config build/webpack.config.build.js --progress",
    "build:csr": "rm -rf dist-csr && cross-env NODE_ENV=production webpack --config build/webpack.config.csr.js --progress",
    "build:ssr": "rm -rf dist && npm run build:ssr:server && npm run build:ssr:client && node w-ssr/utils/rename.mjs",
    "build:ssr:client-a": "cross-env NODE_ENV=production webpack --config build/webpack.config.ssr.client.js --json stats.json --progress --analyze",
    "build:ssr:client": "cross-env NODE_ENV=production webpack --config build/webpack.config.ssr.client.js --progress",
    "build:ssr:server": "cross-env NODE_ENV=production webpack --config build/webpack.config.ssr.server.js --progress",
    "start:ssr": "cross-env SERVER_ENV=production node w-ssr/app.mjs"
  },
  "dependencies": {
    "@cycjimmy/swiper-animation": "^4.1.0",
    "@vue/server-renderer": "^3.0.4",
    "autoprefixer": "^9.8.6",
    "axios": "^0.21.0",
    "core-js": "^3.6.5",
    "json-bigint": "^1.0.0",
    "koa-body": "^4.2.0",
    "koa-static-cache": "^5.1.4",
    "koa2": "^2.0.0-alpha.7",
    "qiankun": "^2.3.4",
    "qs": "^6.9.4",
    "register-service-worker": "^1.7.1",
    "swiper": "^6.4.5",
    "vant": "^3.0.0-rc.3",
    "vue": "^3.0.5",
    "vue-bundle-renderer": "^0.2.2",
    "vue-router": "4.0.0-alpha.11",
    "vue-style-loader": "^4.1.2",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/babel-plugin-jsx": "^1.0.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.4",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "eslint": "^6.7.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "file-loader": "^6.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^1.3.3",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.2.3",
    "postcss-loader": "^4.1.0",
    "postcss-plugin-px2rem": "^0.8.1",
    "postcss-pxtorem": "^5.1.1",
    "prettier": "^1.19.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "~3.9.3",
    "url-loader": "^4.1.1",
    "vite": "^1.0.0-rc.13",
    "vue-jest": "^5.0.0-0",
    "vue-loader": "^16.1.2",
    "vuex-router-sync": "^5.0.0",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.0",
    "webpack-dev-middleware": "^4.0.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^3.0.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/*.{js,jsx,vue,ts,tsx}": [
      "eslint 'src/**/*.{js,vue}'",
      "git add"
    ]
  }
